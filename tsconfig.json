{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "sourceMap": true,
    "outDir": "./dist",
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext", "webworker"],
    "noImplicitAny": true,
    "module": "ES2020",
    "target": "ES2020",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  }
}